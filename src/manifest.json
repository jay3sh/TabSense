{
  "name": "TabSense",
  "version": "1.1",

  "description": "Making sense of Tabs",
  "background_page": "background.html",
  "browser_action":{
    "default_icon" : "images/icon48.png"
  },
  "content_scripts":[{
    "matches" : ["http://*/*","https://*/*","ftp://*/*","file://*/*"],
    "js" : ["scripts/register_key_shortcut.js"],
    "run_at" : "document_start"
  }],
  "permissions": [
    "tabs"
  ]
}
